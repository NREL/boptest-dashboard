{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "baseUrl": ".",
    "esModuleInterop": true,
    "sourceMap": true,
    "module": "commonjs",
    //"target": "es6",
    "target": "es2017",
    "lib": ["es5", "es6", "ES2020.Promise"],
    "moduleResolution": "node",
    "outDir": "./build",
    "noImplicitReturns": false
  },
  "include": ["./**/*.ts", "tests/**/*.ts"],
  "exclude": ["build", "node_modules"]
}
