{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "baseUrl": ".",
    "esModuleInterop": true,
    "sourceMap": true,
    "module": "commonjs",
    //"target": "es6",
    "target": "es2017",
    "lib": ["es5", "es6"],
    "moduleResolution": "node",
    "outDir": "./build",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true
  },
  "include": ["./**/*.ts", "tests/**/*.ts"]
}
